<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.sap.superchargersrl.controller.LoginView"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <Label layoutX="250.0" layoutY="80.0" text="Login"/>
        <Label layoutX="180.0" layoutY="140.0" text="Username"/>
        <TextField fx:id="usernameField" layoutX="250.0" layoutY="140.0"/>
        <Label layoutX="180.0" layoutY="180.0" text="Password"/>
        <PasswordField fx:id="passwordField" layoutX="250.0" layoutY="180.0"/>
        <Button layoutX="250.0" layoutY="230.0" text="Login" onAction="#handleLoginButtonAction"/>
    </children>
</AnchorPane>